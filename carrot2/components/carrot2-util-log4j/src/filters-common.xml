
<!--
    Filtering of logs from common packages and libraries (prevent spamming the output).
-->

<!-- Catalina (Tomcat) -->
<logger name="org.apache.coyote"><level value="info"/></logger>
<logger name="org.apache.catalina"><level value="info"/></logger>
<logger name="org.apache.jasper"><level value="info"/></logger>
<logger name="org.apache.jk"><level value="info"/></logger>
<logger name="org.apache.tomcat"><level value="info"/></logger>

<!-- Apache commons -->
<logger name="org.apache.commons"><level value="warn"/></logger>

<!-- Apache HTTP client -->
<logger name="httpclient"><level value="warn"/></logger>

<!-- Apache Axis -->
<logger name="org.apache.axis"><level value="warn"/></logger>

<!-- JETTY -->
<logger name="org.mortbay"><level value="warn"/></logger>

<!-- EHCache -->
<logger name="net.sf.ehcache"><level value="info"/></logger>

<!-- JDIC -->
<logger name="jdic"><level value="info"/></logger>