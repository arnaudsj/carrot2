
// Create JAR listing files for the PHP script

StringBuffer buf = new StringBuffer();
String base = "lib";
File [] deps = new File(base).listFiles();
for (int i = 0; i < deps.length; i++) {
	if (deps[i].getName().endsWith(".jar")) {
		buf.append("<jar href=\"" + base + "/" + deps[i].getName() + "\" />\n");
	}
}

File out = new File("demo.inc");
new FileOutputStream(out).write(buf.toString().getBytes());