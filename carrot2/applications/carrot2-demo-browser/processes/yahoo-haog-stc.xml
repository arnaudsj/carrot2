<?xml version="1.0" encoding="UTF-8" ?>

<local-process id="yahoo-haog-stc">
  <name>Yahoo Search API -- HAOG-STC Clusterer</name>

  <input  component-key="input-cached-yahooapi" />

  <filter component-key="filter-rawdocument-enumerator" />
  <filter component-key="filter-langset-en" />
  <filter component-key="filter-tokenizer" />
  <filter component-key="filter-smart-case-normalizer" />
  <filter component-key="filter-clustering-haog-stc" />
  <filter component-key="filter-save-xml" />

  <output component-key="output-array" />

  <attribute key="process.settings.class" 
	         value="org.carrot2.demo.settings.HaogStcSettings" />
</local-process>
